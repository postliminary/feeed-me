<h1>Listing entries</h1>

<table>
  <thead>
    <tr>
      <th>Entry</th>
      <th>Feed</th>
      <th>Title</th>
      <th>Url</th>
      <th>Author</th>
      <th>Content</th>
      <th>Summary</th>
      <th>Published</th>
      <th>Updated</th>
      <th>Categories</th>
      <th>Entry</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entries.each do |entry| %>
      <tr>
        <td><%= entry.entry %></td>
        <td><%= entry.feed_id %></td>
        <td><%= entry.title %></td>
        <td><%= entry.url %></td>
        <td><%= entry.author %></td>
        <td><%= entry.content %></td>
        <td><%= entry.summary %></td>
        <td><%= entry.published %></td>
        <td><%= entry.updated %></td>
        <td><%= entry.categories %></td>
        <td><%= entry.entry_id %></td>
        <td><%= link_to 'Show', entry %></td>
        <td><%= link_to 'Edit', edit_entry_path(entry) %></td>
        <td><%= link_to 'Destroy', entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entry', new_entry_path %>
